<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="deltamonstarz.tickettoride.views.gamePlay.ChooseDestinationDialog">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="5dp">


        <Button
            android:id="@+id/destination_card_selection_01"
            android:layout_width="match_parent"
            android:layout_height="@dimen/destination_card_selection_vertical_height"
            android:layout_margin="3dp"
            android:text="---"/>

        <Button
            android:id="@+id/destination_card_selection_02"
            android:layout_width="match_parent"
            android:layout_height="@dimen/destination_card_selection_vertical_height"
            android:layout_margin="3dp"
            android:text="---"/>

        <Button
            android:id="@+id/destination_card_selection_03"
            android:layout_width="match_parent"
            android:layout_height="@dimen/destination_card_selection_vertical_height"
            android:layout_margin="3dp"
            android:text="---"/>





    </LinearLayout>

</FrameLayout>
